{
	"name": "moveData",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "pipe-prop-folder",
						"value": "target1"
					}
				],
				"typeProperties": {
					"source": {
						"type": "CosmosDbMongoDbApiSource",
						"batchSize": 100,
						"filter": "{'name': 'nanda'} "
					},
					"sink": {
						"type": "JsonSink",
						"storeSettings": {
							"type": "AzureDataLakeStoreWriteSettings",
							"copyBehavior": "PreserveHierarchy"
						},
						"formatSettings": {
							"type": "JsonWriteSettings",
							"quoteAllText": true,
							"filePattern": "arrayOfObjects"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "mongoCosmos",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "Json1",
						"type": "DatasetReference",
						"parameters": {
							"fileName": "hello",
							"processTime": {
								"value": "\"@formatDateTime(pipipeline().TriggerTime,    'MM-dd-yyyy_hh-mm-ss')\"",
								"type": "Expression"
							},
							"fileExtention": ".json"
						}
					}
				]
			}
		],
		"parameters": {
			"folder": {
				"type": "string",
				"defaultValue": "pipeParamOneValue"
			}
		},
		"variables": {
			"pipeVarOne": {
				"type": "String",
				"defaultValue": "pipeVarOneValue"
			}
		},
		"annotations": []
	}
}